class Solution {
     
    static int dp[][]=new int[1000][1000];
    static int solve(int i,int j ,String s1 , String s2){
        if(i>=s1.length() || j>=s2.length()){
            return 0;
        }
        if(dp[i][j]!=-1)return dp[i][j];
        if(s1.charAt(i)==s2.charAt(j)){
            return dp[i][j]= 1+ solve(i+1,j+1,s1,s2);
        }else{
            int leftTake=solve(i+1,j,s1,s2);
            int rightTake=solve(i,j+1,s1,s2);
            return dp[i][j]=Math.max(leftTake,rightTake);
        }
    }
    static int bottumUp(String text1, String text2){
        int dp[][]=new int [text1.length()+1][text2.length()+1];
        for(int i=1;i<dp.length;i++){
            for(int j=1;j<dp[0].length;j++){
                if(text1.charAt(i-1)==text2.charAt(j-1)){
                    dp[i][j]=1+dp[i-1][j-1];
                }else{
                    dp[i][j]=Math.max(dp[i-1][j],dp[i][j-1]);
                }
            }
        }
        return dp[text1.length()][text2.length()];
    }
    public int longestCommonSubsequence(String text1, String text2) {
        // for(int i=0;i<dp.length;i++){
        //     Arrays.fill(dp[i],-1);
        // }
        // return solve(0,0,text1,text2);
        return bottumUp(text1,text2);
    }
}
